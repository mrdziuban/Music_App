<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.email %>
      <%= button_to "Delete", user_url(user.id), method: :delete %>
      <% if user.admin == false %>
        <%= button_to "Make admin", adminize_user_url(user.id), method: :get %>
      <% end %>
    </li>
  <% end %>
</ul>